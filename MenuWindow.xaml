<Window x:Class="Gui_client.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Gui_client"
        mc:Ignorable="d"
        Title="Menu" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button Content="Log Out" Margin="10" Click="LogoutButton_Click" />
            <Button Content="Join Room" Margin="10" Click="GetRoomsButton_Click" />
            <Button Content="Create Room" Margin="10" Click="CreateRoomButton_Click" />
        </StackPanel>
    </Grid>
</Window>
